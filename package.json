    "tests": "npm run linter && npm run check-prettier && npm run test -- --coverage --runInBand",
    "build": "npm run build-js && npm run build-less",
    "build-js": "webpack --mode production",
    "build-less": "lessc src/less/index.less dist/css/bundle.css && postcss --no-map --use autoprefixer --use cssnano -r dist/css/bundle.css",
    "watch-js": "webpack --mode development --watch",
    "linter": "eslint '**/*.{ts,tsx}'",
    "check-prettier": "prettier --check '**/*.{ts,tsx}'",
    "prettier": "prettier --write '**/*.{ts,tsx}'"
  },
