    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.3",
    "@types/recharts": "^1.8.16",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
